	gzip -dc mirror/gene_association.paint_fb.gz | ./gaf2obo.pl -s Dmel -n paint_fb > $@.tmp && mv $@.tmp $@


mirror/gene_association.sgn.gz: 
	wget http://geneontology.org/gene-associations/gene_association.sgn.gz -O $@.tmp && mv $@.tmp $@


target/neo-sgn.obo: mirror/gene_association.sgn.gz
	gzip -dc mirror/gene_association.sgn.gz | ./gaf2obo.pl -s Solanaceae -n sgn > $@.tmp && mv $@.tmp $@


mirror/goa_human_complex.gpi.gz: 
	wget ftp://ftp.ebi.ac.uk/pub/databases/GO/goa/HUMAN/goa_human_complex.gpi.gz -O $@.tmp && mv $@.tmp $@


target/neo-goa_human_complex.obo: mirror/goa_human_complex.gpi.gz
	gzip -dc mirror/goa_human_complex.gpi.gz | ./gpi2obo.pl -s Hsap -n goa_human_complex > $@.tmp && mv $@.tmp $@


mirror/gene_association.paint_pombase.gz: 
	wget http://geneontology.org/gene-associations/gene_association.paint_pombase.gz -O $@.tmp && mv $@.tmp $@


target/neo-paint_pombase.obo: mirror/gene_association.paint_pombase.gz
	gzip -dc mirror/gene_association.paint_pombase.gz | ./gaf2obo.pl -s Spom -n paint_pombase > $@.tmp && mv $@.tmp $@


