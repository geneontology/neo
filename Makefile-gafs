mirror/gene_association.paint_wb.gz: 
	wget http://geneontology.org/gene-associations/gene_association.paint_wb.gz -O $@.tmp && mv $@.tmp $@


target/neo-paint_wb.obo: mirror/gene_association.paint_wb.gz
	gzip -dc mirror/gene_association.paint_wb.gz | ./gaf2obo.pl -s Cele -n paint_wb > $@.tmp && mv $@.tmp $@


